DROP TABLE IF EXISTS TB1;
DROP TABLE IF EXISTS TB2;
DROP TABLE IF EXISTS TB3;
DROP TABLE IF EXISTS TB4;
DROP TABLE IF EXISTS TB5;

CREATE TABLE TB1 (id INT PRIMARY KEY NOT NULL, description CHAR(10) NOT NULL);
CREATE TABLE TB2 (id INT PRIMARY KEY NOT NULL, description CHAR(50) NOT NULL);
CREATE TABLE TB3 (id INT PRIMARY KEY NOT NULL, description CHAR(100) NOT NULL);
CREATE TABLE TB4 (id INT PRIMARY KEY NOT NULL, description CHAR(500) NOT NULL);
CREATE TABLE TB5 (id INT PRIMARY KEY NOT NULL, description CHAR(1000) NOT NULL);

